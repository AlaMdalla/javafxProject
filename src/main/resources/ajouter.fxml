<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.AjouterSociete">
   <children>
      <AnchorPane layoutX="10.0" layoutY="10.0" prefHeight="400.0" prefWidth="600.0" styleClass="background" stylesheets="@Ajouter.css">
         <children>
            <VBox layoutX="45.0" layoutY="23.0" prefHeight="185.0" prefWidth="511.0" spacing="10" styleClass="form">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Nom de la société" />
                  <TextField fx:id="fNS" />
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Numéro de téléphone" />
                  <TextField fx:id="fNT" />
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Adresse" />
                  <TextField fx:id="fA" />
               </children>
            </VBox>
            <Button fx:id="btnsave" layoutX="262.0" layoutY="243.0" mnemonicParsing="false" onAction="#createSociete" styleClass="save-button" text="Save" />
            <ImageView fitHeight="30.0" fitWidth="42.0" layoutX="279.0" layoutY="248.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@images/save.png" />
               </image>
            </ImageView>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
