<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.jobflow.controllers.front.societe.AfficherPartenaire">
    <children>
        <VBox alignment="TOP_LEFT" spacing="10" AnchorPane.leftAnchor="10" AnchorPane.rightAnchor="10" AnchorPane.topAnchor="10" AnchorPane.bottomAnchor="10">
            <ListView fx:id="partenaireListView" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
            <HBox spacing="10">
                <TextField fx:id="nomTextField" promptText="Recherche par Nom" />
                <Button fx:id="rechercherParnom" onAction="#rechercherParTitre" text="Search" />
            </HBox>
            <HBox spacing="10">
                <Button fx:id="deleteButton" onAction="#supprimerPartenaire" text="Supprimer" />
                <Button onAction="#trierParMatiere" text="Trier par Nom" />
                <Button onAction="#resetList" text="Réinitialiser" />
                <Button onAction="#goToMainWindow" prefHeight="33.0" prefWidth="143.0" text="Retour à l'accueil" />
            </HBox>
        </VBox>
    </children>
</AnchorPane>
