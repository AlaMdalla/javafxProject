<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Button?>

<AnchorPane prefHeight="434.0" prefWidth="583.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.afficherSociete">
   <children>
      <VBox alignment="TOP_CENTER" spacing="10" AnchorPane.leftAnchor="10" AnchorPane.rightAnchor="10" AnchorPane.topAnchor="10" AnchorPane.bottomAnchor="10">
         <ListView fx:id="societeListView" prefHeight="300.0" prefWidth="500.0" styleClass="societeListView" stylesheets="@Afficher.css" />
         <HBox spacing="10" alignment="CENTER">
            <TextField fx:id="searchField" prefHeight="33.0" prefWidth="300.0" promptText="Recherche par Nom" />
            <Button fx:id="rechercherParNom" onAction="#rechercherParTitre" prefHeight="33.0" prefWidth="100.0" text="Search" />
            <Button onAction="#goToMainWindow" prefHeight="33.0" prefWidth="143.0" text="Retour à l'accueil" />

         </HBox>
         <HBox spacing="10" alignment="CENTER">
            <Button fx:id="deleteButton" onAction="#supprimerSociete" prefHeight="33.0" prefWidth="147.0" styleClass="deleteButton" stylesheets="@Afficher.css" text="Supprimer" />
            <Button onAction="#trierParMatiere" prefHeight="33.0" prefWidth="143.0" text="Trier par Nom" />
            <Button onAction="#resetList" prefHeight="33.0" prefWidth="143.0" text="Réinitialiser" />
         </HBox>
      </VBox>
   </children>
</AnchorPane>
